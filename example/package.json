{
  "name": "authenticare-example",
  "description": "An example of how to use the npm authenticare package",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon server",
    "dev:client": "npm run build -- --watch",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "build": "webpack --config client/webpack.config.js",
    "postinstall": "run-s db:migrate db:seed",
    "test": "jest --verbose tests/client/components tests/server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "authenticare": "^0.4.6",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "knex": "^0.21.0",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.3.0",
    "jsdom": "^15.1.1",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
}
